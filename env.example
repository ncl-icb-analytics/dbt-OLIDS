# Snowflake Connection Configuration
# Copy this file to .env and fill in your actual values
# Never commit .env to version control!

# Snowflake Account (e.g., your-org.eu-west-1 or xy12345)
SNOWFLAKE_ACCOUNT=your-account-here

# Snowflake Username
SNOWFLAKE_USER=your-username-here

# Snowflake Role (Required: ISL-USERGROUP-SECONDEES-NCL for UAT access)
SNOWFLAKE_ROLE=ISL-USERGROUP-SECONDEES-NCL

# Snowflake Warehouse (e.g., NCL_ANALYTICS_XS)
SNOWFLAKE_WAREHOUSE=your-warehouse-here

# Optional: Snowflake Password
# SNOWFLAKE_PASSWORD=your-password-here

# Optional: Production Database (defaults to DATA_LAB_OLIDS_UAT if not set)
# SNOWFLAKE_PROD_DATABASE=DATA_LAB_OLIDS_UAT

# Optional: Source Database (defaults to Data_Store_OLIDS_UAT if not set)
# SNOWFLAKE_SOURCE_DATABASE=Data_Store_OLIDS_UAT

# Optional: Target Database (defaults to DATA_LAB_OLIDS_UAT if not set)
# SNOWFLAKE_TARGET_DATABASE=DATA_LAB_OLIDS_UAT
